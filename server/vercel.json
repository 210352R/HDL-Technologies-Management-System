{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "crons": [
      {
        "path": "https://hdl-technologies-management-system.vercel.app/cron/overdue",
        "schedule": "50 9 * * *"
      }
    ]
  }
  